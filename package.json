{
  "name": "my-react-express-mongo-app",
  "version": "1.0.0",
  "description": "a react test app with mongo configured from scratch",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "mongo"
  ],
  "author": "fabrizio iacovone",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.0",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.19.0",
    "bootstrap": "4.4.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "history": "4.10.1",
    "jest": "24.9.0",
    "md5": "2.2.1",
    "mongodb": "3.4.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.19",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
